{
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.css",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
